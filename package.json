{
  "name": "RNExample",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "ENVFILE=.env.develop && react-native run-android --mode=devDebug",
    "android:stg": "ENVFILE=.env.staging && react-native run-android --mode=stgDebug",
    "android:prod": "ENVFILE=.env.production && react-native run-android --mode=prodDebug",
    "apk:dev": "cd android && ENVFILE=.env.develop ./gradlew assembleDevRelease",
    "apk:stg": "cd android && ENVFILE=.env.staging ./gradlew assembleStgRelease",
    "apk:prod": "cd android && ENVFILE=.env.production ./gradlew assembleProdRelease",
    "aab:stg": "cd android && ENVFILE=.env.staging ./gradlew bundleStgRelease",
    "aab:prod": "cd android && ENVFILE=.env.production ./gradlew bundleProdRelease",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native-stack": "^7.3.10",
    "@sentry/react-native": "^6.11.0",
    "aws-amplify": "^6.14.3",
    "react": "19.0.0",
    "react-native": "0.79.1",
    "react-native-config": "^1.5.5",
    "react-native-paper": "^5.13.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.1",
    "@react-native/eslint-config": "0.79.1",
    "@react-native/metro-config": "0.79.1",
    "@react-native/typescript-config": "0.79.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
